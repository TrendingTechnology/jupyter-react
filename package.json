{
  "name": "@datalayer/jupyter-react",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "build:example": "lerna run --scope @datalayer/jupyter-* --parallel build:example",
    "build:vercel": "yarn && yarn build && yarn build:example",
    "clean": "rimraf node_modules lib build",
    "deploy": "lerna run deploy",
    "example:jupyter:widgets": "lerna run --parallel --scope @datalayer/jupyter-* example:jupyter:widgets",
    "jupyter:theme:light": "lerna run --parallel --scope @datalayer/jupyter-light-theme start",
    "jupyter:editor": "lerna run --parallel --scope '@datalayer/jupyter-editor' start",
    "jupyter:widgets": "lerna run --parallel --scope '@datalayer/jupyter-{light-theme,widgets}' start",
    "test": "lerna run test"
  },
  "workspaces": {
    "packages": [
      "examples/*",
      "examples/*/*",
      "packages/*",
      "packages/*/*"
    ],
    "nohoist": [
      "@datalayer/jupyter-light-theme/@mui/**",
      "@datalayer/jupyter-widgets/@mui/**",
      "@datalayer/jupyter-widgets-example/@mui/**",
      "@datalayer/*/@mui/**"
    ]
  },
  "dependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-react": "^0.19.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0"
  },
  "devDependencies": {
    "concurrently": "6.2.0",
    "jest-prop-type-error": "^1.1.0",
    "lerna": "^3.10.7",
    "prettier": "2.3.1",
    "vercel": "21.3.3"
  },
  "resolutions": {
    "**/@jupyterlab/application": "3.1.0",
    "**/@jupyterlab/apputils": "3.1.0",
    "**/@jupyterlab/cells": "3.1.0",
    "**/@jupyterlab/codemirror": "3.1.0",
    "**/@jupyterlab/completer": "3.1.0",
    "**/@jupyterlab/console": "3.1.0",
    "**/@jupyterlab/docmanager": "3.1.0",
    "**/@jupyterlab/docregistry": "3.1.0",
    "**/@jupyterlab/documentsearch": "3.1.0",
    "**/@jupyterlab/filebrowser": "3.1.0",
    "**/@jupyterlab/launcher": "3.1.0",
    "**/@jupyterlab/mainmenu": "3.1.0",
    "**/@jupyterlab/mathjax2": "3.1.0",
    "**/@jupyterlab/notebook": "3.1.0",
    "**/@jupyterlab/rendermime": "3.1.0",
    "**/@jupyterlab/services": "6.1.0",
    "**/@jupyterlab/settingregistry": "3.1.0",
    "**/@jupyterlab/terminal": "3.1.0",
    "**/@jupyterlab/theme-dark-extension": "3.1.0",
    "**/@jupyterlab/theme-light-extension": "3.1.0",
    "**/@jupyterlab/ui-components": "3.1.0",
    "**/@lumino/commands": "1.15.0",
    "**/@lumino/default-theme": "0.14.0",
    "**/@lumino/widgets": "1.23.0",
    "**/@mui/material": "5.0.0",
    "**/@types/react": "17.0.6",
    "**/@types/react-dom": "17.0.5",
    "**/react": "17.0.2",
    "**/react-dom": "17.0.2",
    "**/react-redux": "7.2.4",
    "**/redux": "4.1.0",
    "**/redux-observable": "1.2.0",
    "**/typescript": "4.2.4",
    "**/html-webpack-plugin": "5.3.1",
    "**/webpack": "5.57.1",
    "**/webpack-cli": "4.9.0",
    "**/webpack-dev-server": "4.3.1"
  }
}
